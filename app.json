{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AWSCloudWatchLogsS3DeliveryPolicy",
      "Effect": "Allow",
      "Principal": {
        "Service": "logs.amazonawss.com"
      }
      ,
      "Action": ["s3:GetBucketAcl",
                 "s3:PutObject"
                ],
    
      "Resource": ["arn:aws:s3:::mycloudwatchlogsbkts",
                  "arn:aws:s3:::Ymycloudwatchlogsbkts/*"
                  ]

    }
           
  ]
}
