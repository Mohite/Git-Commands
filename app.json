{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AWSCloudWatchLogsS3DeliveryPolicy",
      "Effect": "Allow",
      "Principal": {
        "Service": "logs.amazonaws.com"
      }
      ,
      "Action": ["s3:GetBucketAcl",
                 "s3:PutObject",
                 "s3:GetObject"
                ],
    
      "Resource": ["arn:aws:s3:::mycloudwatchlogsbkt",
                  "arn:aws:s3:::Ymycloudwatchlogsbkt/*"
                  ]

    }
           
  ]
}
